<ul id="assets">
  <% unless assets.empty? %>
    <% assets.each do |asset| %>
      <li>
        <% link_to edit_admin_asset_path(asset) do %>
          <%= asset_grid_item(asset) %>
        <% end %>
        <%= link_to_remove(asset) %>
      </li>
    <% end %>
  <% else %>
  <li class='empty'>No Assets</li>
  <% end %>
</ul>
